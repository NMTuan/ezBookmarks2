{
  "name": "ez_bookmarks",
  "description": "无压的浏览器书签管理插件",
  "keywords": [
    "ezbookmark",
    "vue",
    "directus",
    "vite",
    "chrome",
    "extension"
  ],
  "author": "NMTuan",
  "license": "MIT",
  "homepage": "https://github.com/NMTuan/ezBookmarks2",
  "private": true,
  "version": "2.0.0",
  "scripts": {
    "dev": "vite build --mode developmemnt",
    "build:dev": "vite build --mode developmemnt",
    "build:prod": "vite build",
    "serve": "vite preview",
    "watch": "npm-watch"
  },
  "dependencies": {
    "axios": "^1.1.3",
    "throttle-debounce": "^5.0.0",
    "vue": "^3.0.5"
  },
  "devDependencies": {
    "@asigloo/prettier-config": "^0.2.0",
    "@unocss/transformer-compile-class": "^0.46.2",
    "@unocss/transformer-directives": "^0.46.2",
    "@vitejs/plugin-vue": "^1.3.0",
    "@vue/compiler-sfc": "^3.0.5",
    "npm-watch": "^0.11.0",
    "sass": "^1.55.0",
    "unocss": "^0.46.2",
    "vite": "^2.4.4",
    "vite-plugin-chrome-extension": "^0.0.7"
  },
  "watch": {
    "build:dev": {
      "patterns": [
        "src"
      ],
      "extensions": "css,js,json,vue,html",
      "runOnChangeOnly": false
    }
  }
}
